CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_xiaomi_redmi-router-ac2100=y
CONFIG_LUCI_LANG_zh_Hans=y

CONFIG_KERNEL_MIPS_FPU_EMULATOR=y

CONFIG_PACKAGE_luci-app-ramfree=y   # 释放内存
CONFIG_PACKAGE_luci-app-frpc=y  # 内网穿透 Frp(client)
CONFIG_PACKAGE_luci-app-jd-dailybonus=y 
CONFIG_PACKAGE_luci-app-shadowsocks-libev=y
CONFIG_PACKAGE_luci-app-ssr-plus=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Shadowsocks_Libev_Client=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_V2ray_Plugin=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Xray=y
CONFIG_PACKAGE_luci-app-ttyd=y  # 网页终端命令行
CONFIG_PACKAGE_luci-app-upnp=y  # 通用即插即用 UPnP（端口自动转发）
CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_OFFLOADING=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_BBR_CCA=y
CONFIG_PACKAGE_luci-app-webadmin=y      # Web 管理页面设置
CONFIG_PACKAGE_luci-app-smartdns=y  # smartdns工具
CONFIG_PACKAGE_luci-app-unblockmusic=y   # 解锁网易云灰色歌曲3合1新版本
CONFIG_PACKAGE_luci-app-autoreboot=y    # 支持计划重启
CONFIG_PACKAGE_luci-app-filetransfer=y # 文件传输（可web安装ipk包）  
CONFIG_PACKAGE_luci-app-vlmcsd=y	# KMS 激活服务器
CONFIG_PACKAGE_luci-app-vsftpd=y	# FTP 服务器
CONFIG_PACKAGE_luci-app-arpbind=y	# IP/MAC 绑定
CONFIG_PACKAGE_luci-app-firewall=y	# 防火墙
CONFIG_PACKAGE_luci-app-nlbwmon=y   # 网络带宽监视器
CONFIG_PACKAGE_luci-app-wrtbwmon=y  # 实时流量监测
CONFIG_PACKAGE_luci-app-netdata=y   # Netdata 实时图表监控
CONFIG_PACKAGE_luci-app-statistics=y    #流量监控工具
CONFIG_PACKAGE_luci-theme-argon=y       # argon主题

CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_wget=y
CONFIG_PACKAGE_tcpdump=y
CONFIG_PACKAGE_zsh=y
CONFIG_PACKAGE_bash=y

# add ipv6 support
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_ipv6helper=y
CONFIG_PACKAGE_iputils-traceroute6=y

# 其他需要安装的软件包:
CONFIG_OPENSSL_PREFER_CHACHA_OVER_GCM=y
CONFIG_PACKAGE_autocore-switch=y

CONFIG_PACKAGE_luci-compat=y
